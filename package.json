{
  "name": "founders-names",
  "description": "Get random founder's names",
  "version": "0.0.0-semantic-release",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "test": "istanbul cover -x *.test.js _mocha -- -R spec",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cameronjroe/founders-names.git"
  },
  "keywords": [
    "random",
    "founders"
  ],
  "author": "Cameron J Roe <cameronjroe@gmail.com> (http://cameronjroe.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cameronjroe/founders-names/issues"
  },
  "homepage": "https://github.com/cameronjroe/founders-names#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.5.0",
    "cz-conventional-changelog": "1.1.5",
    "ghooks": "1.0.3",
    "istanbul": "0.4.2",
    "mocha": "2.4.5",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
